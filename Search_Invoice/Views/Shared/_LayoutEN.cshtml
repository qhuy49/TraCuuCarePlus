    <script type="text/javascript">
        function abc() {
            bootbox.confirm("<form id='infos' action=''>\
            <label >Old password     :</label><input type='text' name='password' /><br/>\
            <label >New password    :</label><input type='text' name='newpassword' /><br/>\
            <lable>Confirm new password:</lable><input type='text' name='renewpassword' />\
            </form>", function (result) {
                    if (result) {
                        var jk = $("#infos").serialize();
                        console.log(jk);

                        $.ajax({
                            url: "/AccountEn/ChangePassword_Use",
                            type: "POST",
                            data: jk,
                            success: function (result) {

                                result = JSON.parse(result);
                                if (!result.hasOwnProperty("error")) {

                                    bootbox.alert("Change password successfully !");
                                    $.get("/AccountEn/Logoff", function (data) {
                                    });
                                }
                                else {
                                    bootbox.alert(result.error);
                                }
                            }
                        });
                    }
                });


        }
