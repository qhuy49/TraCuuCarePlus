    <script type="text/javascript">
        function abc() {
            bootbox.confirm("<form id='infos' action=''>\
        <label >Mật khẩu cũ     :</label><input type='text' name='password' /><br/>\
        <label >Mật khẩu mới    :</label><input type='text' name='newpassword' /><br/>\
        <lable>Nhập lại mật khẩu:</lable><input type='text' name='renewpassword' />\
        </form>", function (result) {
        if (result) {
            var jk = $("#infos").serialize();
            console.log(jk);

            $.ajax({
                url: "/Account/ChangePassword_Use",
                type: "POST",
                data: jk,
                success: function (result) {

                    result = JSON.parse(result);
                    if (!result.hasOwnProperty("error")) {

                        bootbox.alert("Đổi mật khẩu thành công !");
                        $.get("/Account/Logoff", function (data) {
                        });
                    }
                    else {
                        bootbox.alert(result.error);
                    }
                }
            });
        }
    });

        
